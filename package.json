{
  "name": "coop-main",
  "version": "1.0.4",
  "description": "Single server community bot",
  "type": "module",
  "scripts": {
    "start": "pm2 kill && yarn start-bot && yarn start-api",
    "start-bot": "pm2 start --name bot bot.mjs",
    "start-api": "pm2 start --name api api.mjs",
    "dev-bot": "pm2 start bot.mjs && pm2 logs",
    "dev": "nodemon ./index.mjs",
    "lint": "eslint ."
  },
  "author": "lmf-git",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "coop-api": "https://github.com/the-coop/coop-api.git#a42ac94c6fe34b64b5a2f07822dcfa7f9eece6ab",
    "coop-bot": "https://github.com/the-coop/coop-bot.git#bdb7f7df8a0e4af963ae556b7157334f8f99be32"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "nodemon": "^2.0.7"
  },
  "homepage": "https://thecoop.group"
}
