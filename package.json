{
  "name": "coop-main",
  "version": "1.0.4",
  "description": "Single server community bot",
  "type": "module",
  "scripts": {
    "start": "pm2 kill && yarn start-bot && yarn start-api",
    "start-bot": "pm2 start --name bot bot.mjs",
    "start-api": "pm2 start --name api api.mjs",
    "dev-bot": "pm2 start bot.mjs && pm2 logs",
    "dev": "nodemon ./index.mjs",
    "lint": "eslint ."
  },
  "author": "lmf-git",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "coop-api": "https://github.com/the-coop/coop-api.git#1525977ad224d75b8b41f61388a027f83abcb2d4",
    "coop-bot": "https://github.com/the-coop/coop-bot.git#ab8a4b03263d6f6d5550724014908e2886b8c63e"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "nodemon": "^2.0.7"
  },
  "homepage": "https://thecoop.group"
}
