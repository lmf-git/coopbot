{
  "name": "coop-main",
  "version": "1.0.4",
  "description": "Single server community bot",
  "type": "module",
  "scripts": {
    "start": "pm2 kill && yarn start-bot && yarn start-api",
    "start-bot": "pm2 start --name bot bot.mjs",
    "start-api": "pm2 start --name api api.mjs",
    "dev-bot": "pm2 start bot.mjs && pm2 logs",
    "dev": "nodemon ./index.mjs",
    "lint": "eslint ."
  },
  "author": "lmf-git",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "coop-api": "https://github.com/the-coop/coop-api.git#751a8702d6fe0435fe5aff93f9d708bf01511122",
    "coop-bot": "https://github.com/the-coop/coop-bot.git#50ecd32e159b6810fa8c4e1d2a50e6b106e74df2"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "nodemon": "^2.0.7"
  },
  "homepage": "https://thecoop.group"
}
