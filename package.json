{
  "name": "coop-main",
  "version": "1.0.4",
  "description": "Single server community bot",
  "type": "module",
  "scripts": {
    "start": "pm2 kill && yarn start-bot && yarn start-api",
    "start-bot": "pm2 start --name bot bot.mjs",
    "start-api": "pm2 start --name api api.mjs",
    "dev-bot": "pm2 start bot.mjs && pm2 logs",
    "dev": "nodemon ./index.mjs",
    "lint": "eslint ."
  },
  "author": "lmf-git",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "coop-api": "https://github.com/the-coop/coop-api.git#a90611b9a22ef9800e8090add8337ab7c456e2df",
    "coop-bot": "https://github.com/the-coop/coop-bot.git#3f80067d569ab6d934b2cb97fde3288564fd4590"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "nodemon": "^2.0.7"
  },
  "homepage": "https://thecoop.group"
}
