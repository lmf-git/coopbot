{
  "name": "coop-main",
  "version": "1.0.4",
  "description": "Single server community bot",
  "type": "module",
  "scripts": {
    "start": "pm2 kill && yarn start-bot && yarn start-api",
    "start-bot": "pm2 start --name bot bot.mjs",
    "start-api": "pm2 start --name api api.mjs",
    "dev-bot": "pm2 start bot.mjs && pm2 logs",
    "dev": "nodemon ./index.mjs",
    "lint": "eslint ."
  },
  "author": "lmf-git",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "coop-api": "https://github.com/the-coop/coop-api.git#b0b5b82c0ec844078eb3ebf2443c89ac7ea27fc8",
    "coop-bot": "https://github.com/the-coop/coop-bot.git#a9c1ffcb4b3ef7ee98b01046541e649f36fb6d7d"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "nodemon": "^2.0.7"
  },
  "homepage": "https://thecoop.group"
}
